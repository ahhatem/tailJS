{
  "root@10.10.10.50":
  [
    {
      "fileName": "/var/log/SmartsSamplelogs.log",
      "fileDescription": "EMC Smarts logs",
      "logFormateRegex": "(\\[\\w+ \\d*, \\d{1,4} \\d*:\\d*:\\d* \\w+ \\w+ \\W\\w+]) ([^\\[]*)",
      "logFormateScope":"g",
      "GlobalFilterRegex": ".*?",
      "EventMap":
      [
        {
          "filterName"  : "filter_Error",
          "filterRegex" : "error",
          "timeStampRegex": "default:now",
          "eventNameRegex": "default:CriticalSymptom",
          "elementNameRegex": "default:10.10.10.50",
          "instanceNameRegex": "default:CRM_ApplicationService",
          "classNameRegex": "default:SoftwareService",
          "severityRegex": "default:Critical"
        },

        {
          "filterName"  : "filter_messi",
          "filterRegex" : "messi",
          "timeStampRegex": "default:now",
          "eventNameRegex": "default:MinorSymptom",
          "elementNameRegex": "default:MesseiryTestLab",
          "instanceNameRegex": "default:CRM_ApplicationService",
          "classNameRegex": "default:SoftwareService",
          "severityRegex": "default:Critical"
        },
        {
          "filterName"  : "filter_URL",
          "filterRegex" : "url",
          "timeStampRegex": "default:now",
          "eventNameRegex": "default:CriticalSymptom",
          "elementNameRegex": "'(.*?)'",
          "instanceNameRegex": "default:CRM_ApplicationService",
          "classNameRegex": "default:SoftwareService",
          "severityRegex": "default:Critical"
        },
        {
          "filterName"  : "filter_Critical",
          "filterRegex" : "Not Error",
          "timeStampRegex": "default:now",
          "eventNameRegex": "default:CriticalSymptom",
          "elementNameRegex": "default:10.10.10.50",
          "instanceNameRegex": "default:CRM_ApplicationService",
          "classNameRegex": "default:SoftwareService",
          "severityRegex": "default:Critical"
        }
      ]
    },
    {
      "fileName": "/var/log/alert_ASM1.log",
      "fileDescription": "the first file",
      "logFormateRegex": "^([a-z]{1,3} [a-z]{1,3} \\d{1,2} \\d{1,2}:\\d{1,2}:\\d{1,2} \\d{4})\r?\n(.*(?:\r?\n(?![a-z]{1,3} [a-z]{1,3} \\d{1,2} \\d{1,2}:\\d{1,2}:\\d{1,2} \\d{4}).*)*)",
      "logFormateScope":"gmi",
      "GlobalFilterRegex": ".*?",
      "EventMap":
      [
        {
          "filterName"  : "filter_Error2",
          "filterRegex" : "error",
          "timeStampRegex": "default:now",
          "eventNameRegex": "default:CriticalSymptom",
          "elementNameRegex": "default:10.10.10.50",
          "instanceNameRegex": "default:CRM_ApplicationService",
          "classNameRegex": "default:SoftwareService",
          "severityRegex": "default:Critical"
        },
        {
          "filterName"  : "filter_URL",
          "filterRegex" : "url",
          "timeStampRegex": "default:now",
          "eventNameRegex": "default:CriticalSymptom",
          "elementNameRegex": "'(.*?)'",
          "instanceNameRegex": "default:CRM_ApplicationService",
          "classNameRegex": "default:SoftwareService",
          "severityRegex": "default:Critical"
        },
        {
          "filterName"  : "filter_Critical",
          "filterRegex" : "Not Error",
          "timeStampRegex": "default:now",
          "eventNameRegex": "default:CriticalSymptom",
          "elementNameRegex": "default:10.10.10.50",
          "instanceNameRegex": "default:CRM_ApplicationService",
          "classNameRegex": "default:SoftwareService",
          "severityRegex": "default:Critical"
        }
      ]
    }
  ]
